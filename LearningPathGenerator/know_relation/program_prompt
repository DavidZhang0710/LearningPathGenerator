class Rel:
    def __init__(self, name: str):
        self.name = name
class Precursor(Rel):#先修关系
    def __init__(self, name: str):
        super.__init__(name=name)

class Knowledge:
    def __init__(self, name: str):
        self.name = name

class Triple:
    def __init__(self, head: Knowledge, relation: Rel, tail: Knowledge):
        self.head = head
        self.relation = relation
        self.tail = tail
        
class Relations:
    def __init__(self, triples: List[Triple] = []):
        self.triples = triples